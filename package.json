{
  "name": "@pyyupsk/fdu",
  "version": "0.0.0-beta.1",
  "description": "Ultra-fast, zero-dependency JavaScript date-time library",
  "keywords": [
    "date",
    "time",
    "datetime",
    "format",
    "parse",
    "lightweight",
    "zero-dependency",
    "fast",
    "dayjs",
    "alternative"
  ],
  "license": "MIT",
  "author": "Pongsakorn Thipayanate <contact@fasu.dev>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./locale": {
      "types": "./dist/locale.d.ts",
      "import": "./dist/locale.js",
      "require": "./dist/locale.cjs"
    },
    "./locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "bench": "vitest bench --run",
    "build": "rimraf dist && tsdown",
    "docs:build": "cd docs && bun run build",
    "docs:dev": "cd docs && bun run dev",
    "docs:preview": "cd docs && bun run start",
    "format": "biome format --write",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/bun": "^1.2.23",
    "@vitest/coverage-v8": "^3.2.4",
    "dayjs": "^1.11.18",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
